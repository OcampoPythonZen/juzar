{
  "name": "modern-landing-page",
  "version": "1.0.0",
  "description": "Modern landing page with 3D model integration for WordPress",
  "scripts": {
    "dev": "vite",
    "start": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "preview:prod": "vite preview --port 4173 --host",
    "build:prod": "cross-env NODE_ENV=production vite build --mode production",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js --fix",
    "lint:css": "stylelint '**/*.css' --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "update-deps": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.15.19",
    "@types/three": "^0.176.0",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "postcss": "^8.4.35",
    "postcss-browser-reporter": "^0.7.0",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-nesting": "^13.0.1",
    "postcss-preset-env": "^10.1.6",
    "postcss-reporter": "^7.1.0",
    "postcss-sorting": "^9.1.0",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-style-import": "^2.0.0",
    "vitest": "^3.1.4"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "private": true
}
